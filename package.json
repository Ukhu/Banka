    "@babel/register": "^7.4.0",
    "bcrypt": "^3.0.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
