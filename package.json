{
  "name": "banka",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking\r operations like account creation, customer deposit and withdrawals.",
  "main": "app.js",
  "scripts": {
    "start": "babel-node API/app.js",
    "dev": "SET NODE_ENV=development nodemon --exec babel-node API/app.js",
    "test": "SET NODE_ENV=test && nyc --reporter=lcov --reporter=text --register @babel/register mocha test --recursive --exit",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ukhu/Banka.git"
  },
  "author": "Osaukhumwen Iyamuosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ukhu/Banka/issues"
  },
  "homepage": "https://github.com/Ukhu/Banka#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
